{
    "name": "autoznetwork/autoznetwork-php",
    "description": "autoZnetwork PHP Package",
    "keywords": [
        "autoznetwork",
        "autoznetwork-php"
    ],
    "homepage": "https://github.com/autoznetwork/autoznetwork-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Collin Pedersen",
            "email": "collin@autoznetwork.com",
            "homepage": "https://www.autoznetwork.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "guzzlehttp/guzzle": "~6.0",
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.11"
    },
    "suggest": {
        "guzzlehttp/guzzle": "Allows for implementation of the Guzzle HTTP client"
    },
    "autoload": {
        "psr-4": {
            "AutozNetwork\\": "src/AutozNetwork/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AutozNetwork\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
